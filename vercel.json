{
  "builds": [
    {
      "src": "text-buddy/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "migo-backend/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    "routes": [
  { "src": "/api/chat", "dest": "migo-backend/api/echo" }
  { "src": "/(.*)", "dest": "text-buddy/$1" }
]
}
